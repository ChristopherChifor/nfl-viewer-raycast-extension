{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "nfl-viewer",
  "title": "NFL Viewer",
  "description": "NFL Viewer will display the NFL live schedule",
  "icon": "command-icon.png",
  "author": "ChrisC",
  "categories": [
    "Fun",
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "nfl",
      "title": "nfl",
      "subtitle": "View the NFL schedule",
      "description": "\"nfl\" will display the latest nfl schedule.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.46.0",
    "@raycast/utils": "^1.4.16",
    "axios": "^1.2.3"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
